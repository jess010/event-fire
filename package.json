{
    "name": "event-fire",
    "version": "1.0.0",
    "description": "An automated tool for testing user interaction with events on a web page",
    "main": "index.js",
    "scripts": {
        "build-client": "webpack",
        "build-client-watch": "webpack -w",
        "deploy": "git checkout -b deploy && webpack -p && git add -f public/bundle.js public/bundle.js.map && git commit --allow-empty -m 'Deploying' && git push --force heroku deploy:master && git checkout master && git branch -D deploy",
        "start": "node server",
        "start-dev": "npm run build-client-watch & npm run start-server",
        "start-server": "NODE_ENV='development' nodemon server -e html,js,scss --ignore public",
        "test": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/boilermaker' mocha ./**/*.spec.js --compilers js:babel-register",
        "seed": "node seed.js"
    },
    "author": "Jessica Freasier",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.15.3",
        "babel-preset-react": "^6.24.1",
        "bluebird": "^3.5.0",
        "body-parser": "^1.16.1",
        "bootstrap": "^3.3.7",
        "connect-session-sequelize": "^4.1.0",
        "express": "^4.14.1",
        "express-session": "^1.15.1",
        "fs": "0.0.1-security",
        "ghostdriver": "^2.0.0",
        "history": "^4.6.3",
        "ignore-loader": "^0.1.2",
        "morgan": "^1.8.1",
        "net": "^1.0.2",
        "path": "^0.12.7",
        "pg": "^6.1.2",
        "pg-hstore": "^2.3.2",
        "prop-types": "^15.5.8",
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-redux": "^5.0.2",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.1.1",
        "react-table": "^6.7.4",
        "redux": "^3.6.0",
        "redux-logger": "^2.8.1",
        "redux-thunk": "^2.2.0",
        "selenium-webdriver": "^3.5.0",
        "sequelize": "^4.3.1",
        "socket.io": "^2.0.3",
        "tls": "0.0.1",
        "watchify": "^3.9.0"
    },
    "devDependencies": {
